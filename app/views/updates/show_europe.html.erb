<%= render 'regional' %>

<div class = "row">
	<h1>Europe Headlines</h1>
</div>

<div class = "row">
	<% @europenews["articles"].each do |u| %>
		<h3><%= u["title"] %></h3> <br>
		<h4 class = "subheader"><%= u["publish_date"] %></h4><br>
		<p><%= u["summary"] %><p><br>
		<a href= "#"><%= link_to nil, u["url"] %><br></a>
	<% end %>
</div>